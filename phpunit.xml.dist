<?xml version="1.0"?>
<phpunit
    bootstrap="vendor/autoload.php"
    colors="true"
>
    <filter>
        <whitelist>
            <directory suffix=".php">src</directory>
        </whitelist>
    </filter>
    <testsuites>
        <testsuite name="OAuth">
            <directory suffix="Test.php">test</directory>
        </testsuite>
    </testsuites>
</phpunit>
